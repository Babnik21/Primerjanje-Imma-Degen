button.icon.path = "M16,8 L14,8 L14,16 L10,16 L10,10 L6,10 L6,16 L2,16 L2,8 L0,8 L8,0 L16,8 Z M16,8";
button.marginBottom = 5;
button.parent = chart8.zoomControl;
button.insertBefore(chart8.zoomControl.plusButton);
</script>
