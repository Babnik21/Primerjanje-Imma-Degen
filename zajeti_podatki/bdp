<script>
var container1 = am4core.create("chartdiv1", am4core.Container);
container1.width = am4core.percent(100);
container1.height = am4core.percent(100);
container1.layout = "vertical";

var legendContainer = container1.createChild(am4core.Container);
legendContainer.width = am4core.percent(100);
legendContainer.height = 100;
legendContainer.zIndex = 100;
legendContainer.background.fill = am4core.color("#ffffff");

var label = legendContainer.createChild(am4core.Label);
label.text = "Projected GDP (Nominal) per capita 2021";
label.fontSize = 20;
label.align = "center";

function addRec(x,y,color,text) {
var rect = legendContainer.createChild(am4core.Rectangle);
rect.width = 15;
rect.height = 15;
rect.x = x;
rect.y = y;
rect.fill = am4core.color(color);
var label = legendContainer.createChild(am4core.Label);
label.text = text;
label.fontSize = 15;
label.align = "center";
label.isMeasured = false;
label.x = (x+18);
label.y = y;
}

addRec(150,30,"#006600","Above 100000 $");
addRec(300,30,"#00b300","75000-100000 $");
addRec(450,30,"#79d279","50000-75000 $");
addRec(600,30,"#94b8b8","25000-50000 $");
addRec(750,30,"#ffc34d","10000-25000 $");
addRec(150,50,"#e085c2","5000-10000 $");
addRec(300,50,"#ff9999","3000-5000 $");
addRec(450,50,"#e60000","1000-3000 $");
addRec(600,50,"#990000","Below 1000 $");

var chart1 = container1.createChild(am4maps.MapChart);
chart1.width = am4core.percent(100);
chart1.height = 500;

chart1.geodata = am4geodata_worldHigh;
chart1.projection = new am4maps.projections.Miller();

var polygonSeries = chart1.series.push(new am4maps.MapPolygonSeries());
polygonSeries.dataSource.url = "../m/economy/json/projected-nominal-capita-map.json";

polygonSeries.useGeodata = true;
polygonSeries.exclude = ["AQ","KP","SO","SY","RE","YT","EH","SH","FO","GG","IM","AD","GI","VA","LI","MC",
	"BQ","AI","CU","CW","GP","KY","MQ","MS","BL","MF","SX","TC","VG","VI","BM","GL","PM","FK","GF","NC","GU","MP","AS","CK","NU","PF","TK","WF","AX","JE","JU","GO","IO","CC","CX","NF","UM-DQ","UM-FQ","UM-HQ","UM-JQ","UM-MQ","UM-WQ","SJ","PN","GS","BV","TF","HM",];
polygonSeries.nonScalingStroke = true;
polygonSeries.strokeWidth = 3.5;
polygonSeries.calculateVisualCenter = true;

var polygonTemplate = polygonSeries.mapPolygons.template;
polygonTemplate.tooltipText = "{rank}. {name}[/]\nGDP per capita : ${nominal}\n% of world : {npercent}%";
polygonTemplate.propertyFields.fill = "color";

var hs = polygonTemplate.states.create("hover");
hs.propertyFields.fill = "color";

var hs = polygonTemplate.states.create("hover");
hs.propertyFields.fill = "color";

var polygonSeries1 = chart1.series.push(new am4maps.MapPolygonSeriesh());
polygonSeries1.useGeodata = true;
polygonSeries1.include = ["KP","SO","SY","RE","YT","EH","SH","FO","GG","IM","AD","GI","VA","LI","MC",
"BQ","AI","CU","CW","GP","KY","MQ","MS","BL","MF","SX","TC","VG","VI","BM","GL","PM","FK","GF","NC","GU","MP","AS","CK","NU","PF","TK","WF","AX","JE","JU","GO","IO","CC","CX","NF","UM-DQ","UM-FQ","UM-HQ","UM-JQ","UM-MQ","UM-WQ","SJ","PN","GS","BV","TF","HM",];
polygonSeries1.mapPolygons.template.tooltipText = "{name}[/] : no data";
  
chart1.zoomControl = new am4maps.ZoomControl();
chart1.zoomControl.slider.height = 100;
chart1.zoomControl.marginBottom = 100;
var button = chart1.chartContainer.createChild(am4core.Button);
button.padding(5, 5, 5, 5);
button.marginRight = 15;
button.events.on("hit", function() {
  chart1.goHome();
});
button.icon = new am4core.Sprite();
button.icon.path = "M16,8 L14,8 L14,16 L10,16 L10,10 L6,10 L6,16 L2,16 L2,8 L0,8 L8,0 L16,8 Z M16,8";
