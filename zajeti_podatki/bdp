<script>
var container3 = am4core.create("chartdiv3", am4core.Container);
container3.width = am4core.percent(100);
container3.height = am4core.percent(100);
container3.layout = "vertical";

var legendContainer = container3.createChild(am4core.Container);
legendContainer.width = am4core.percent(100);
legendContainer.height = 100;
legendContainer.zIndex = 100;
legendContainer.background.fill = am4core.color("#ffffff");

var label = legendContainer.createChild(am4core.Label);
label.text = "Projected GDP (Nominal) per capita 2021";
label.fontSize = 20;
label.align = "center";

function addRec(x,y,color,text) {
var rect = legendContainer.createChild(am4core.Rectangle);
rect.width = 15;
rect.height = 15;
rect.x = x;
rect.y = y;
rect.fill = am4core.color(color);
var label = legendContainer.createChild(am4core.Label);
label.text = text;
label.fontSize = 15;
label.align = "center";
label.isMeasured = false;
label.x = (x+18);
label.y = y;
}

addRec(150,30,"#006600","Above 100000 $");
addRec(300,30,"#00b300","75000-100000 $");
addRec(450,30,"#79d279","50000-75000 $");
addRec(600,30,"#94b8b8","25000-50000 $");
addRec(750,30,"#ffc34d","10000-25000 $");
addRec(150,50,"#e085c2","5000-10000 $");
addRec(300,50,"#ff9999","3000-5000 $");
addRec(450,50,"#e60000","1000-3000 $");
addRec(600,50,"#990000","Below 1000 $");

var chart3 = container3.createChild(am4maps.MapChart);
chart3.width = am4core.percent(100);
chart3.height = 500;

chart3.geodata = am4geodata_worldHigh;
chart3.projection = new am4maps.projections.Miller();
var polygonSeries = chart3.series.push(new am4maps.MapPolygonSeries());

// Make map load polygon (like country names) data from GeoJSON
polygonSeries.useGeodata = true;
polygonSeries.exclude = ["AQ"];
polygonSeries.nonScalingStroke = true;
polygonSeries.strokeWidth = 0.5;
polygonSeries.calculateVisualCenter = true;

var imageSeries = chart3.series.push(new am4maps.MapImageSeries());
imageSeries.dataSource.url = "../m/economy/json/projected-nominal-capita-map.json";
imageSeries.dataFields.value = "nominal";

var imageTemplate = imageSeries.mapImages.template;
imageTemplate.nonScaling = true;

var circle = imageTemplate.createChild(am4core.Circle);
circle.fillOpacity = 0.7;
circle.propertyFields.fill = "color";
circle.tooltipText = "{rank}. {name}[/]\nGDP per capita : ${nominal}\n% of world : {npercent}%";

imageSeries.heatRules.push({
  "target": circle,
  "property": "radius",
  "min": 4,
  "max": 30,
  "dataField": "value"
});

imageTemplate.adapter.add("latitude", function(latitude, target) {
  var polygon = polygonSeries.getPolygonById(target.dataItem.dataContext.id);
  if(polygon){
    return polygon.visualLatitude;
   }
   return latitude;
});

imageTemplate.adapter.add("longitude", function(longitude, target) {
  var polygon = polygonSeries.getPolygonById(target.dataItem.dataContext.id);
  if(polygon){
    return polygon.visualLongitude;
   }
   return longitude;
});
  
chart3.zoomControl = new am4maps.ZoomControl();
chart3.zoomControl.slider.height = 100;
chart3.zoomControl.marginBottom = 100;
var button = chart3.chartContainer.createChild(am4core.Button);
button.padding(5, 5, 5, 5);
button.marginRight = 15;
button.events.on("hit", function() {
  chart3.goHome();
});
button.icon = new am4core.Sprite();
button.icon.path = "M16,8 L14,8 L14,16 L10,16 L10,10 L6,10 L6,16 L2,16 L2,8 L0,8 L8,0 L16,8 Z M16,8";
button.marginBottom = 5;
button.parent = chart3.zoomControl;
button.insertBefore(chart3.zoomControl.plusButton);
</script>

