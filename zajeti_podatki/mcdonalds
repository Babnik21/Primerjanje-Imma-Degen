<div class="row">             
	<div class="col-md-12">	
		<hr/><h3>Comments</h3>
		<div id="disqus_thread"></div>
		<script>
		var disqus_config = function () { 
		  this.language = "en";
		};
		(function() { // DON'T EDIT BELOW THIS LINE
		var d = document, s = d.createElement('script');
		s.src = 'https://atlasbig.disqus.com/embed.js';
		s.setAttribute('data-timestamp', +new Date());
		(d.head || d.body).appendChild(s);
		})();
		</script>
		<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
	</div>
</div>


    <script>
	
        $("#mapid").html("");
        $("#mapid").css("padding-top", "80vh");

		$('input[type=radio][name=dataSelection]').change(function() {
			prepareVisualization(this.value);
		});

		
        $('#dataTable').on('click', '.data-row', function(event) {
            $(this).addClass('active').siblings().removeClass('active');
        });

        $('#dataTable').on('click', '.data-row', function(event) {
        if($(this).hasClass('active')){
            $(this).removeClass('active'); 
        } else {
            $(this).addClass('active').siblings().removeClass('active');
        }
        });

        var mymap = L.map('mapid').fitBounds([[-180,-80],[180,80]]);
        
        var info = L.control();
        info.onAdd = function (map) {
            this._div = L.DomUtil.create('div', 'info'); // create a div with a class "info"
            this.update();
            return this._div;
        };

        // method that we will use to update the control based on feature properties passed
        info.update = function (props) {
            if(props)
            this._div.innerHTML = 
            '<h4></h4>' +  (props ?
                '<b>' + props.name + '</b><br />' + props.relData
                : 'Hover over a state');
            
        };

        info.addTo(mymap);

        var done = 3; 
        var areas;
        var areaNames;
        var dataSet;
        var featuresMapping = new Object();

        $( document ).ready(function() {

            shiftColumnsFormattedNumbers = 2;
                                    columnsFormattedNumbers = [shiftColumnsFormattedNumbers];

            $('#data-table').DataTable({
                paging: false,
                info: false,
                "columnDefs": [
                    { type: 'formatted-num', targets: columnsFormattedNumbers }
                ],
                oLanguage: {
                    "sSearch": ""
                }
            });

            // AreaSet
            zip.createReader(new zip.HttpReader("/cdn/administrative-area-sets/cf2ff005-8f80-4118-b4fb-a306e56cb45d/areas.json.zip"), function(reader) {
                reader.getEntries(function(entries) {
                if (entries.length) {
                    entries[0].getData(new zip.TextWriter(), function(text) {
                    areas = JSON.parse(text);
                    done-=1;
                    if(done == 0) 
                        prepareVisualization(0);
                    // close the zip reader
                    reader.close(function() {
                        // onclose callback
                    });
                    }, function(current, total) {
                    // onprogress callback
                    });
                }
                });
            }, function(error) {
                console.log( "Unable to unzip areas");
            });

            // Area Names
            zip.createReader(new zip.HttpReader("/cdn/administrative-area-sets/cf2ff005-8f80-4118-b4fb-a306e56cb45d/names.json.zip"), function(reader) {
                reader.getEntries(function(entries) {
                if (entries.length) {
                    entries[0].getData(new zip.TextWriter(), function(text) {
                    areaNames = JSON.parse(text);
                    done-=1;
                    if(done == 0) 
                        prepareVisualization(0);
                    reader.close(function() {});
                    }, function(current, total) {});
                }
                });
            }, function(error) {
                console.log( "Unable to unzip area names. " + error);
            });

            // AreasDataSet
            zip.createReader(new zip.HttpReader("/cdn/administrative-area-set-data/296eda88-c175-476c-8b37-6dec57e7e4ce/dataset.json.zip"), function(reader) {
                reader.getEntries(function(entries) {
                if (entries.length) {
                    entries[0].getData(new zip.TextWriter(), function(text) {
                    dataSet = JSON.parse(text);
                    done-=1;
                    if(done == 0) 
                        prepareVisualization(0);
                    // close the zip reader
                    reader.close(function() {
                        // onclose callback
                    });
                    }, function(current, total) {
                    // onprogress callback
                    });
                }
                });
            }, function(error) {
                console.log( "Unable to unzip areas");
            });

        });


        function onEachFeature(feature, layer) {
            layer.on({
                mouseover: highlightFeature,
                mouseout: resetHighlight,
                click: zoomToFeature
            });
            featuresMapping[feature.properties.ref] = {
                feature: feature, layer: layer
            };
        }

        function highlightFeature(e) {
            
            var layer = e.target;

            layer.setStyle({
                weight: 5,
                color: '#666',
                dashArray: '',
                fillOpacity: 0.8
            });

            if (!L.Browser.ie && !L.Browser.opera && !L.Browser.edge) {
                layer.bringToFront();
            }

            info.update(layer.feature.properties);
            
        }

        var geojson;
        var minData;
        var maxData;
		var medianData;
		var logShift;
        var allData;

        function prepareVisualization(selectedDataIndex) {

		    allData = []; 
            areas.features.forEach(element => {
                    for (var key in dataSet.data) {
                        if(key == element.properties['ref'] && dataSet.data[key][selectedDataIndex]!= null) {
                            element.properties.relData   = dataSet.data[key][selectedDataIndex];
                            allData.push(dataSet.data[key][selectedDataIndex]);
                        }
                    }
                });	
            allData.sort(function(a,b){return a - b});
			
			minData = allData[0];
			maxData = allData[allData.length-1];
			medianData = allData[Math.floor(allData.length/2)];
			logShift = (medianData * medianData - minData * maxData ) / (2 * medianData - minData - maxData);
			if( (minData - logShift) < 0 ){
				logShift = (minData - 1);
			}
			
            areas.features.forEach(element => {
				if(areaNames['primary'][element.properties['ref']].en != null)
					element.properties.name   = areaNames['primary'][element.properties['ref']].en
				else
					element.properties.name   = areaNames['primary'][element.properties['ref']].default;    
            });

			mymap.eachLayer(function (layer) {
				mymap.removeLayer(layer);
			});
			
			L.tileLayer('https://api.mapbox.com/styles/v1/turaner/{id}/tiles/256/{z}/{x}/{y}?access_token=pk.eyJ1IjoidHVyYW5lciIsImEiOiJjajd2dzRtZGkwZnYxMnFvMHV6NG1tMno5In0.7sA2pU61eg7WkHOSIienoA', {
				maxZoom: 18,
				id: 'cjljpkts7298v2rqrwq8t9jcj'
			}).addTo(mymap);
			
            geojson = L.geoJson(areas.features, {
                style: style,
                onEachFeature: onEachFeature
            }).addTo(mymap);

            var multipolygon = L.geoJson(areas);
            mymap.fitBounds(multipolygon.getBounds());
        }

        

        function resetHighlight(e) {
            geojson.resetStyle(e.target);
            info.update();
        }

        function zoomToFeature(e) {
            mymap.fitBounds(e.target.getBounds());
        }

        function style(feature) {
            return {
                weight: 0,
                opacity: 1,
                color: 'white',
                dashArray: '3',
                fillOpacity: 0.7,
                fillColor: getColor(feature.properties.relData)
            };
        }   

        function getColor(d) {

            min = minData;
            max = maxData;
            if(d== null){
                return dataSet.style.defaultColor;
            }

            if(d>=minData && d<=maxData) {
			
                                    d = Math.log(d - logShift);
                    max = Math.log(max- logShift);
                    min = Math.log(min- logShift);
                
                                    normalized = (d-min)/(max-min);
                
                position = normalized * (dataSet.style.colors.length -1);

                                    w = position - Math.floor(position)
                    c1 = dataSet.style.colors[Math.floor(position)];
                    if(w == 0)
                        return c1;
                    c2 = dataSet.style.colors[Math.floor(position)+1];
                    return rgbWeightedAverage(c1,c2,w)
                                
            }
            else {
                return dataSet.style.defaultColor;
            }
        }

        function rgbWeightedAverage(c1, c2, w) {
            col1 = hexToRgb(c1);
            col2 = hexToRgb(c2);
            r = w * (col2.r - col1.r) + col1.r;
            g = w * (col2.g - col1.g) + col1.g;
            b = w * (col2.b - col1.b) + col1.b;
            return "#" + componentToHex(r) + componentToHex(g) + componentToHex(b);
        }

        function hexToRgb(hex) {
            var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
            return result ? {
                r: parseInt(result[1], 16),
                g: parseInt(result[2], 16),
                b: parseInt(result[3], 16)
            } : null;
        }

        function rgbToHex(r, g, b) {                
            return "#" + componentToHex(r) + componentToHex(g) + componentToHex(b);
        }

        function componentToHex(c) {
            var hex = Math.round(c).toString(16);
            return hex.length == 1 ? "0" + hex : hex;
        }  

        function zoomMap(ref) {
            $('html, body').animate({scrollTop: $("#mapid").offset().top}, 500);
            mymap.fitBounds(L.geoJson(featuresMapping[ref].feature).getBounds());
            featuresMapping[ref].layer.openPopup();
        }

    </script>

	

	

<div class="row" id="footer" style="background-color:#008744; margin-top:15px;padding-top:25px;">
    <div class="col-md-4">
				
    </div>
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
    </div>
  </div>
  
  <div class="row" style="background-color:#008744;">
