                                            <div class="img-placeholder" data-breed="baby">
                            <figure>
                                <noscript>
    <img src="/assets/images/default/breed-sheet-cat.jpg" class="breed-mainImage-default" alt="Tiffany" />
</noscript>
<img
    src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="
